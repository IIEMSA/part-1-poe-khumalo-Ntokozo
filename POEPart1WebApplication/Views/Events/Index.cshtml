@model IEnumerable<Events>

<h2>Events</h2>
<table class="table">
    <thead>
        <tr>
            <th>Event Name</th>
            <th>Event Date</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var eventItem in Model)
        {
            <tr>
                <td>@eventItem.EventName</td>
                <td>@eventItem.EventDate.ToString("yyyy-MM-dd")</td>
                <td>@eventItem.Description</td>
            </tr>
        }
    </tbody>
</table>
